
# Take only barcode02 and 03, ignore 01 as containing non-fastq file
├── case01
│   ├── barcode01
│   │   ├── garbage.txt
│   │   ├── reads0.fastq
│   │   ├── reads1.fastq
│   │   └── reads2.fastq
│   ├── barcode02
│   │   ├── reads0.fastq
│   │   ├── reads1.fastq
│   │   └── reads2.fastq
│   └── barcode03
│       └── reads0.fastq

# Take barcode02, barcode03, and garbage. Ignore barcode01 as case01
├── case02
│   ├── barcode01
│   │   ├── garbage.txt
│   │   ├── reads0.fastq
│   │   ├── reads1.fastq
│   │   └── reads2.fastq
│   ├── barcode02
│   │   ├── reads0.fastq
│   │   ├── reads1.fastq
│   │   └── reads2.fastq
│   ├── barcode03
│   │   └── reads0.fastq
│   └── garbage
│       └── reads1.fastq

# Take case03
├── case03
│   ├── reads0.fastq
│   └── reads1.fastq

# Take all
